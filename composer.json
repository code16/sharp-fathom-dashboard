{
    "name": "code16/sharp-fathom-dashboard",
    "description": "Sharp package that adds a fathom dashboard",
    "keywords": [
        "Code16",
        "laravel",
        "sharp-fathom-dashboard"
    ],
    "homepage": "https://github.com/code16/sharp-fathom-dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucien PUGET",
            "email": "lucien.puget@code16.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "code16/sharp": "^9.0",
        "illuminate/contracts": "^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Code16\\SharpFathomDashboard\\": "src/",
            "Code16\\SharpFathomDashboard\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\SharpFathomDashboard\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\SharpFathomDashboard\\SharpFathomDashboardServiceProvider"
            ],
            "aliases": {
                "SharpFathomDashboard": "Code16\\SharpFathomDashboard\\Facades\\SharpFathomDashboard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
